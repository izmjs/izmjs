{
  "files.eol": "\n",
  "files.exclude": {
    "db/**": true
  },
  "eslint.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": ["javascript"],
  "json.schemas": [
    {
      "fileMatch": ["/modules/*/variables.meta.json"],
      "url": "https://raw.githubusercontent.com/izmjs/izmjs/develop//schemas/variables.meta.json"
    }
  ]
}
